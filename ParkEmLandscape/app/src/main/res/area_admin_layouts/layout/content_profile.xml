            <!--&lt;!&ndash;android:name="com.google.android.gms.maps.SupportMapFragment"&ndash;&gt;-->
            <!--&lt;!&ndash;android:layout_width="match_parent"&ndash;&gt;-->
            <!--&lt;!&ndash;android:layout_height="match_parent"/>&ndash;&gt;-->
        <!---->


    <!--</LinearLayout>-->
    <fragment
        android:id="@+id/admin_profile_map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="100dp" />


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="3">

            <TableRow
                android:layout_weight="1"
                android:weightSum="4">
                <TextView
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text=""/>
                <TextView
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text="slots"
                    android:gravity="center"/>
                <TextView
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text="price for 1 hr"
                    android:gravity="center"/>
                <TextView
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text="price after 1 hr"
                    android:gravity="center"/>
            </TableRow>

            <TableRow
                android:layout_weight="1"
                android:weightSum="4">
                <TextView
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text="2 wheeler"
                    android:gravity="center"/>
                <EditText
                    android:id="@+id/textViewProfileTwoWheelerSlots"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text=""
                    android:ems="5"
                    android:inputType="phone"
                    android:gravity="center"/>
                <EditText
                    android:id="@+id/textViewProfileTwoWheelerPriceForOneHour"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text=""
                    android:inputType="phone"
                    android:ems="5"
                    android:gravity="center"/>
                <EditText
                    android:id="@+id/textViewProfileTwoWheelerPriceAfterOneHour"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:inputType="phone"
                    android:layout_weight="1"
                    android:text=""
                    android:ems="5"
                    android:gravity="center"/>
            </TableRow>

            <TableRow
                android:layout_weight="1"
                android:weightSum="4">
                <TextView
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text="4 wheeler"
                    android:gravity="center"/>
                <EditText
                    android:id="@+id/textViewProfileFourWheelerSlots"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:inputType="phone"
                    android:text=""
                    android:ems="5"
                    android:gravity="center"/>
                <EditText
                    android:id="@+id/textViewProfileFourWheelerPriceForOneHour"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:inputType="phone"
                    android:text=""
                    android:ems="5"
                    android:gravity="center"/>
                <EditText
                    android:id="@+id/textViewProfileFourWheelerPriceAfterOneHour"
                    android:layout_height="match_parent"
                    android:inputType="phone"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text=""
                    android:ems="5"
                    android:gravity="center"/>
            </TableRow>
        </TableLayout>
    </LinearLayout>

    <Button
        android:id="@+id/buttonUpadteAdminProfile"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Update" />

</LinearLayout>
